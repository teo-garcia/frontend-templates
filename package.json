{
  "name": "frontend-templates",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "nx run-many --target=dev --parallel=2",
    "dev:next": "nx run react-template-next:dev",
    "dev:rr": "nx run react-template-rr:dev",
    "build": "nx run-many --target=build --parallel=2",
    "build:next": "nx run react-template-next:build",
    "build:rr": "nx run react-template-rr:build",
    "lint": "nx run-many --target=lint --parallel=2",
    "lint:next": "nx run react-template-next:lint",
    "lint:rr": "nx run react-template-rr:lint",
    "test": "nx run-many --target=test --parallel=2",
    "test:next": "nx run react-template-next:test",
    "test:rr": "nx run react-template-rr:test",
    "clean": "nx reset && rm -rf node_modules apps/*/node_modules packages/*/node_modules",
    "prepare": "husky"
  },
  "devDependencies": {
    "@nx/next": "21.2.1",
    "@nx/react": "21.2.1",
    "@nx/vite": "21.2.1",
    "@nx/workspace": "21.2.1",
    "nx": "21.2.1",
    "typescript": "5.8.3",
    "eslint": "9.28.0",
    "prettier": "3.5.3",
    "jest": "29.7.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/user-event": "14.6.1",
    "@testing-library/dom": "10.4.0",
    "@playwright/test": "1.52.0",
    "@types/jest": "29.5.14",
    "jest-environment-jsdom": "29.7.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.0",
    "concurrently": "9.1.2"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "@tanstack/react-query": "5.80.6",
    "@tanstack/react-query-devtools": "5.80.6",
    "clsx": "2.1.1",
    "lucide-react": "0.513.0"
  }
}
